{
  "expo": {
    "name": "isiaccess",
    "scheme": "isiaccess",
    "slug": "isiaccess",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/isi.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "package": "com.youpi.ISIAccess"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/isi.png",
        "backgroundColor": "#ffffff"
      },
      "plugins": ["@notifee/react-native"],
      "package": "com.youpi.ISIAccess"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1a83eae0-21b1-45cb-9fba-1c6d1c946b5f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1a83eae0-21b1-45cb-9fba-1c6d1c946b5f"
    }
  }
}
